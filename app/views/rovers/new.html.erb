<%= form_for :rover_params, url: calculate_path do |f| %>
  Initial X: <%= f.number_field :xCoord %><br />
  Initial Y: <%= f.number_field :yCoord %><br />
  Direction: <%= f.text_field :direction %><br />
  Instructions    : <%= f.text_field :instructions %><br />
  <%= f.submit %>
<% end %>

<div data-controller="hello">
  <h1 data-target="hello.output"></h1>
</div>